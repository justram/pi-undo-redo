{
	"name": "@justram/pi-undo-redo",
	"version": "0.2.0",
	"description": "Buffered undo/redo extension for Pi coding agent",
	"keywords": [
		"pi-package",
		"pi",
		"pi-coding-agent",
		"extension",
		"undo-redo"
	],
	"author": "Jheng-Hong Yang",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/justram/pi-undo-redo.git"
	},
	"bugs": {
		"url": "https://github.com/justram/pi-undo-redo/issues"
	},
	"homepage": "https://github.com/justram/pi-undo-redo#readme",
	"type": "module",
	"files": [
		"*.ts",
		"skills/**",
		"README.md",
		"LICENSE",
		"package.json",
		"tsconfig.json"
	],
	"pi": {
