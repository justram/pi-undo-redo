	"devDependencies": {
		"@biomejs/biome": "2.3.5",
		"@mariozechner/pi-coding-agent": "^0.51.2",
		"@mariozechner/pi-tui": "^0.51.2",
		"@types/diff": "^5.0.9",
		"@types/node": "^22.10.5",
